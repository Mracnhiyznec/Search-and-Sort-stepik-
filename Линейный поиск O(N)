
#python
n = int(input())
s = [int(i) for i in input().split()]
x = int(input())
counter = 0
for i in range(len(s)):
    if s[i] == x:
        counter+=1
print(counter)


#C++
#include <iostream>
using namespace std;
int main(){
    int a, x, counter = 0;
    cin >> a;
    int array[a];
    for (int i = 0; i < a; i++){
        cin >> array[i];
    }
    cin >> x;
    for (int i = 0; i < a; i++){
        if (array[i] == x){
            counter++;
        }
    }
    cout << counter;
    return 0;
}
